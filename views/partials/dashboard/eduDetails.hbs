<h2 class="my-2">Education Credentials</h2>
<table class="table">
  <thead>
    <tr>
      <th>School</th>
      <th class="hide-sm">Degree</th>
      <th class="hide-sm">Years</th>
      <th />
    </tr>
  </thead>
  <tbody>
    {{#each edu}}
      <tr>
        <td>{{school}}</td>
        <td class="hide-sm">{{degree}}</td>
        <td class="hide-sm">
          {{from}}
          -
          {{to}}
        </td>
        <td>
          <button class="btn btn-danger">
            Delete
          </button>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>